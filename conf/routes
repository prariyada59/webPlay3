# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /header                     controllers.Application.header()
GET     /iPhone                     controllers.Application.iPhone()
GET     /Huawei                     controllers.Application.Huawei()
GET     /Oppo                       controllers.Application.Oppo()
GET     /SamSung                    controllers.Application.SamSung()
GET     /Vivo                       controllers.Application.Vivo()
GET     /order                      controllers.Application.order()
GET     /show                       controllers.Application.show()

GET     /products_form                 controllers.Application.products_form()
POST    /product_post                  controllers.Application.product_post()

#Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
